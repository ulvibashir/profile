# Database Configuration
POSTGRES_URL=postgres://username:password@hostname.region.provider.com/database_name
POSTGRES_URL_NON_POOLING=postgres://username:password@hostname.region.provider.com/database_name?sslmode=require

# Email Configuration (Resend)
RESEND_API_KEY=re_123456789abcdefghijklmnopqrstuvwxyz
NOTIFICATION_FROM_EMAIL=contact@yourdomain.com
NOTIFICATION_TO_EMAIL=your-email@example.com

# Authentication (NextAuth)
NEXTAUTH_SECRET=generate_a_strong_random_secret_at_least_32_chars
NEXTAUTH_URL=https://yourdomain.com

# Analytics IP Geolocation
IPINFO_TOKEN=ipinfo_token_12345abcdef

# Google Analytics (Optional)
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-ABCDEFGHIJ

# CDN/Image Optimization (Optional)
NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=your_cloud_name
NEXT_PUBLIC_CLOUDINARY_API_KEY=123456789012345
CLOUDINARY_API_SECRET=abcdefghijklmnopqrstuvwxyz12345

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_EMAIL_NOTIFICATIONS=true